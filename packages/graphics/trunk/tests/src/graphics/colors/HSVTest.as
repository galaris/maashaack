new HSV() ;            assertNotNull( hsv , "01-01 - Constructor failed." ) ;            assertEquals(hsv.h , 0 , "01-02 Constructor failed with the h parameter.") ;            assertEquals(hsv.s , 0 , "01-03 Constructor failed with the s parameter.") ;            assertEquals(hsv.v , 0 , "01-04 Constructor failed with the v parameter.") ;                        hsv = new HSV(180,1,0.5) ;            assertNotNull( hsv , "02-01 - Constructor failed." ) ;            assertEquals(hsv.h , 180 , "02-02 Constructor failed with the h parameter.") ;            assertEquals(hsv.s , 1   , "02-03 Constructor failed with the s parameter.") ;            assertEquals(hsv.v , 0.5 , "02-04 Constructor failed with the v parameter.") ;        }                public function testInterface():void        {            var hsv:HSV = new HSV() ;            assertTrue( hsv is ColorSpace , "HSV must implements the ColorSpace interface.") ;        }                public function testH():void        {            var hsv:HSV = new HSV() ;                        assertEquals(hsv.h , 0 , "01 - h property failed.") ;                        hsv.h = 180 ;            assertEquals(hsv.h , 180 , "02 - h property failed.") ;                        hsv.h = 255.5 ;            assertEquals(hsv.h , 255.5 , "03 - h property failed.") ;                        hsv.h = 360 ;            assertEquals(hsv.h , 0 , "04 - h property failed.") ;                        hsv.h = -10 ;            assertEquals(hsv.h , 350 , "05 - h property failed.") ;                        hsv.h = 370 ;            assertEquals(hsv.h , 10 , "06 - h property failed.") ;                        hsv.h = NaN ;            assertEquals(hsv.h , 0 , "07 - h property failed with the NaN value.") ;        }                public function testS():void        {            var hsv:HSV = new HSV() ;                        assertEquals(hsv.s , 0 , "01 - s property failed.") ;                        hsv.s = 1 ;            assertEquals(hsv.s , 1 , "02 - s property failed.") ;                        hsv.s = 0.5 ;            assertEquals(hsv.s , 0.5 , "03 - s property failed.") ;                        hsv.s = NaN ;            assertEquals(hsv.s , 0 , "04 - s property failed with the NaN value.") ;                        hsv.s = -10 ;            assertEquals(hsv.s , 0 , "05 - s property failed with negative value.") ;                        hsv.s = 10 ;            assertEquals(hsv.s , 1 , "06 - s property failed with >1 value.") ;        }                public function testV():void        {            var hsv:HSV = new HSV() ;                        assertEquals(hsv.v , 0 , "01 - v property failed.") ;                        hsv.v = 1 ;            assertEquals(hsv.v , 1 , "02 - v property failed.") ;                        hsv.v = 0.5 ;            assertEquals(hsv.v , 0.5 , "03 - v property failed.") ;                        hsv.v = NaN ;            assertEquals(hsv.v , 0 , "04 - v property failed with the NaN value.") ;                        hsv.v = -10 ;            assertEquals(hsv.v , 0 , "05 - v property failed with negative value.") ;                        hsv.v = 10 ;            assertEquals(hsv.v , 1 , "06 - v property failed with >1 value.") ;        }                public function testClone():void        {            var hsv:HSV = new HSV(180,1,0.5) ;            var clone:HSV = hsv.clone() as HSV ;                         assertNotNull( clone , "01 - clone method failed." ) ;                        assertEquals(hsv.h , clone.h , "02-01 clone method failed.") ;            assertEquals(hsv.s , clone.s , "02-02 clone method failed.") ;            assertEquals(hsv.v , clone.v , "02-03 clone method failed.") ;                        assertEquals(hsv , clone , "03 clone method failed, use the equals method.") ;        }                public function testEquals():void        {            var hsv1:HSV = new HSV(180,1,0.5) ;            var hsv2:HSV = new HSV(180,1,0.5) ;            var hsv3:HSV = new HSV(190,1,0.5) ;            var hsv4:HSV = new HSV(180,0.4,0.5) ;            var hsv5:HSV = new HSV(180,1,0.6) ;                        assertTrue( hsv1.equals(hsv1) , "01 equals method failed.") ;            assertTrue( hsv1.equals(hsv2) , "02 equals method failed.") ;                        assertFalse( hsv1.equals(hsv3) , "03 equals method failed.") ;            assertFalse( hsv1.equals(hsv4) , "04 equals method failed.") ;            assertFalse( hsv1.equals(hsv5) , "05 equals method failed.") ;        }                public function testSet():void        {            var hsv:HSV = new HSV() ;            hsv.set(180,1,0.5) ;                        assertEquals(hsv.h , 180 , "01 - set method failed.") ;            assertEquals(hsv.s , 1   , "02 - set method failed.") ;            assertEquals(hsv.v , 0.5 , "03 - set method failed.") ;        }                public function testToObject():void        {            var hsv:HSV = new HSV(180,1,0.5) ;            var object:Object = hsv.toObject() as Object;                         assertNotNull( object , "01 - toObject method failed." ) ;                        assertEquals(hsv.h , object.h , "02-01 toObject method failed.") ;            assertEquals(hsv.s , object.s , "02-02 toObject method failed.") ;            assertEquals(hsv.v , object.v , "02-03 toObject method failed.") ;                        assertFalse( hsv.equals(object) , "03 toObject method failed.") ;        }                public function testToSource():void        {            var hsv:HSV = new HSV(180,1,0.5) ;            assertEquals(hsv.toSource() , "new graphics.colors.HSV(180,1,0.5)" , "toSource method failed.") ;        }                public function testToString():void        {            var hsv:HSV = new HSV(180,1,0.5) ;            assertEquals(hsv.toString() , "[HSV h:180 s:1 v:0.5]" , "toString method failed.") ;        }    }}